#! RHF Interaction Energy test for water dimer (DirectDFJ + LinK)

molecule mol {
  0 1
  O  -1.551007  -0.114520   0.000000
  H  -1.934259   0.762503   0.000000
  H  -0.599677   0.040712   0.000000
  --
  0 1
  O   1.350625   0.111469   0.000000
  H   1.680398  -0.373741  -0.758561
  H   1.680398  -0.373741   0.758561
  symmetry c1
  no_reorient
  no_com
}

set {
  scf_type direct
  df_scf_guess false
  basis jun-cc-pVDZ
  e_convergence 1.0e-10
  screening density
  incfock true
  ints_tolerance 1.0e-12
  linK_ints_tolerance 1.0e-12
}

ref_energy = energy('scf', bsse_type='nocp')

set {
  scf_type composite
  basis jun-cc-pVDZ
  e_convergence 1.0e-10
  screening density
  incfock true
  ints_tolerance 1.0e-12
  linK_ints_tolerance 1.0e-12
}

test_energy = energy('scf', bsse_type='nocp')
psi4.compare_values(ref_energy, test_energy, 6, "SCF Int Energy (DirectDFJ+LinK)")
